<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
     <LinearLayout
            android:id="@+id/lay_prolist"
            android:layout_width="fill_parent"
            android:layout_height="50dp" >

            <LinearLayout
                android:id="@+id/lay_map_back"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/resourse_images_back" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <TextView
                    android:id="@+id/tv_act_mapname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical|right" >

            
            </LinearLayout>
        </LinearLayout>
    
    <WebView 
        android:id="@+id/mapfrawebview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <WebView 
        android:id="@+id/mapfrawebview2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <WebView 
        android:id="@+id/mapfrawebview3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
</LinearLayout>
